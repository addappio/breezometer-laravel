{
    "name": "nwidart/breezometer-laravel",
    "description": "Laravel implementation of the Breezometer-php client",
    "keywords": [
        "Breezometer",
        "Air",
        "Pollution",
        "Laravel",
        "breezometer-laravel"
    ],
    "homepage": "https://github.com/nwidart/breezometer-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Widart",
            "email": "n.widart@gmail.com",
            "homepage": "http://www.nicolaswidart.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.4.0",
        "illuminate/support": "4.*",
        "nwidart/breezometer-php": "1.0.*@dev"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "orchestra/testbench": "2.2.*"
    },
    "autoload": {
        "psr-4": {
            "Nwidart\\BreezometerLaravel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nwidart\\BreezometerLaravel\\tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
