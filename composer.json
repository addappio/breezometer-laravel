{
    "name": "addapp/breezometer-laravel",
    "description": "Laravel implementation of the Breezometer-php client",
    "keywords": [
        "Breezometer",
        "Air",
        "Pollution",
        "Laravel",
        "breezometer-laravel"
    ],
    "homepage": "https://github.com/addappio/breezometer-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Widart",
            "email": "n.widart@gmail.com",
            "homepage": "http://www.nicolaswidart.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.4.0",
        "illuminate/support": "~5.0",
        "addapp/breezometer-php": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Addapp\\BreezometerLaravel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Addapp\\BreezometerLaravel\\tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
